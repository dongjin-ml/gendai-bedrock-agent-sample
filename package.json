{
  "name": "simple_br",
  "version": "0.1.0",
  "bin": {
    "simple_br": "bin/simple_br.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk:bootstrap": "cdk bootstrap",
    "deploy": "npm run login:ecrpublic && cdk deploy --all",
    "destroy": "cdk destroy --all",
    "login:ecrpublic": "aws ecr-public get-login-password --region us-east-1 | docker login --username AWS --password-stdin public.ecr.aws",
    "test": "jest",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "^2.145.0-alpha.0",
    "@cdklabs/generative-ai-cdk-constructs": "^0.1.176",
    "@langchain/core": "^0.2.9",
    "@types/js-yaml": "^4.0.9",
    "aws-cdk": "2.145.0",
    "aws-cdk-lib": "2.145.0",
    "cdk-nag": "^2.28.140",
    "constructs": "^10.0.0",
    "js-yaml": "^4.1.0"
  }
}
